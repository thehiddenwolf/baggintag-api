{
  "name": "baggintag-api",
  "type": "module",
  "version": "0.0.1",
  "description": "an app for building up data to tag and train stable diffusion",
  "main": "index.mjs",
  "scripts": {
    "run-api": "cross-env NODE_OPTIONS=--experimental-vm-modules node index.mjs",
    "debug-api": "cross-env NODE_OPTIONS=--experimental-vm-modules node --inspect-brk index.mjs",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules npx jest",
    "debug-test": "cross-env NODE_OPTIONS=--experimental-vm-modules node --inspect-brk ./node_modules/jest/bin/jest.js",
    "lint": "npx eslint . --ext .js,.mjs",
    "lint-fix": "npx eslint . --ext .js,.mjs --fix"

  },
  "author": "The hidden wolf",
  "license": "ISC",
  "dependencies": {
    "ffmpeg": "^0.0.4",
    "imagemagick": "^0.1.3",
    "mariadb": "^3.1.0",
    "sequelize": "^6.29.3"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.21.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.36.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "jest": "^29.5.0",
    "mock-fs": "^5.2.0",
    "ts-node": "^10.9.1"
  }
}
